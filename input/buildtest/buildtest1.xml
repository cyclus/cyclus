<?xml version="1.0"?>
<!-- 1 BuildRegion and 1 BuildInst -->
<!-- 1 SourceFacility FrontEnd, 1 SinkFacility BackEnd, and 1 GreedyMarket -->
<simulation>
  <duration>10</duration>
  <startmonth>1</startmonth>
  <startyear>2000</startyear>
  <simstart>0</simstart>
  <decay>2</decay>

  <commodity>
    <name>uo2</name>
  </commodity>

  <market>
    <name>uo2market</name>
    <mktcommodity>uo2</mktcommodity>
    <model>
      <NullMarket/>
    </model>
  </market>

  <facility>
    <name>FrontEnd</name>
    <model>
      <SourceFacility>
        <outcommodity>uo2</outcommodity>
        <capacity>1</capacity>
        <inventorysize>1e4</inventorysize>
        <commodprice>0</commodprice>
        <recipe>fresh_50gwd</recipe>
      </SourceFacility>
    </model>
  </facility>

  <facility>
    <name>BackEnd</name>
    <model>
      <SinkFacility>
        <incommodity>uo2</incommodity>
        <capacity>1e3</capacity>
        <inventorysize>1e4</inventorysize>
        <commodprice>0</commodprice>
      </SinkFacility>
    </model>
  </facility>

  <region>
    <name>oneRegion</name>
    <allowedfacility>FrontEnd</allowedfacility>
    <allowedfacility>BackEnd</allowedfacility>
    <model>
      <BuildRegion>
	<prototyperequirement>
	  <prototypename>FrontEnd</prototypename>
	  <demandschedule>
	    <entry>
	      <time>0</time>
	      <number>1</number>
	    </entry>
	  </demandschedule>
	</prototyperequirement>
	<prototyperequirement>
	  <prototypename>BackEnd</prototypename>
	  <demandschedule>
	    <entry>
	      <time>0</time>
	      <number>1</number>
	    </entry>
	  </demandschedule>
	</prototyperequirement>
      </BuildRegion>
    </model>
    <institution>
      <name>oneInst</name>
      <model>
        <BuildInst>
	  <availableprototype>FrontEnd</availableprototype>
	  <availableprototype>BackEnd</availableprototype>
        </BuildInst>
      </model>
    </institution>
  </region>

  <recipe>
    <name>fresh_50gwd</name>
    <basis>mass</basis>
    <isotope>
      <id>92235</id>
      <comp>0.05</comp>
    </isotope>
    <isotope>
      <id>92238</id>
      <comp>0.95</comp>
    </isotope>
    <isotope>
      <id>08016</id>
      <comp>0.13</comp>
    </isotope>
  </recipe>

</simulation>
